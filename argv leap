#include<stdio.h>
int main(int argc,char* argv[])
{
    int a;
    a=atoi(argv[1]);
    if(a%400==0)
    {
        if(a%100==0)
        {
            printf("leap year");
        }
        else
        {
            printf("non leap year");
        }}
    else if(a%4==0)
    {
        printf("leap year");
    }
    else
    {
        printf("non leap year");
    }
    return 0;
    
}
